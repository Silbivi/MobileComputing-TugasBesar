<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.silvia.cookieandmilk">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/cookies"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/cookies"
        android:supportsRtl="true"
        android:theme="@style/Theme.CookieandMilk">
        <activity android:name="com.silvia.cookieandmilk.contact" />
        <activity android:name="com.silvia.cookieandmilk.menu" />
        <activity android:name="com.silvia.cookieandmilk.review" />
        <activity android:name="com.silvia.cookieandmilk.Pesan" />
        <activity android:name="com.silvia.cookieandmilk.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>